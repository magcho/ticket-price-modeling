{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "trailing-comma": false,
    "no-console": false,
    "semicolon": [true, "never"],
    "no-unused-expression": false,
    "interface-name": false,
    "variable-name": {
      "options": ["allow-leading-underscore"]
    },
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
